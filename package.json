{
  "name": "lineupjs",
  "description": "LineUp is an interactive technique designed to create, visualize and explore rankings of items based on a set of heterogeneous attributes.",
  "version": "1.0.0",
  "author": {
    "name": "Caleydo Team",
    "email": "contact@caleydo.org",
    "url": "https://caleydo.org"
  },
  "contributors": [
    {
      "name": "Samuel Gratzl",
      "email": "samuel-gratzl@gmx.at"
    },
    {
      "name": "Marc Streit",
      "email": "marc@streit.com"
    },
    {
      "name": "Holger Stitz",
      "email": "kontakt@holgerstitz.de"
    },
    {
      "name": "Hendrik Strobelt",
      "email": "hstrobelt@seas.harvard.edu"
    }
  ],
  "engines": {
    "npm": ">= 3",
    "node": ">= 6",
    "iojs": ">= 3"
  },
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/Caleydo/lineup.js/issues"
  },
  "homepage": "https://github.com/Caleydo/lineup.js",
  "main": "dist/LineUpJS.js",
  "files": [
    "build",
	"!build/docs",
	"src/**/*.ts",
	"src/**/*.scss"
  ],
  "scripts": {
    "compile": "tsc",
    "lint": "tslint -c tslint.json src/**.ts tests/**.ts",
    "docs": "cp -r typedoc_hacked ./node_modules/typedoc/bin/typedoc && typedoc --options typedoc.json src",
    "pretest": "npm run compile",
    "test": "echo 'no tests'",
    "posttest": "npm run lint",
    "prebuild": "npm run test",
    "build": "webpack",
    "watch": "webpack --watch",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags",
    "predist": "npm run build && npm run docs",
    "dist": "mkdirp dist && cd build && tar cvzf ../dist/lineupjs.tar.gz *",
	"prepublish": "npm run dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Caleydo/lineup.js.git"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^2.2.4",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "mkdirp": "^0.5.1",
    "node-sass": "^3.10.1",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "tslint": "^3.15.1",
    "typedoc": "^0.5.1",
    "typescript": "2.1.1",
    "webpack": "^2.1.0-beta.25"
  },
  "optionalDependencies": {
    "@types/react": "^0.14.42",
    "@types/react-dom": "^0.14.18",
    "react": "^15.0.0",
    "react-dom": "^15.0.0"
  },
  "dependencies": {
    "@types/d3": "^3.5.36",
    "d3": "^3.5.17",
    "font-awesome": "^4.6.3"
  }
}
